main(){
  print("*** Program Grade ***");
  int nilai = 92;
  String grade;
  if(nilai >= 95) grade = "A+";
  else if(nilai >= 85) grade = "A";
  else if(nilai >= 75) grade = "B+";
  else if(nilai >= 65) grade = "B";
  else if(nilai >= 55) grade = "C+";
  else if(nilai >= 45) grade = "C";
  else if(nilai >= 35) grade = "D";
  else if(nilai >= 25) grade = "E";
  else grade = "F";
  print("Grade: $grade");

}
