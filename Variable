void main(){
    // membaut variabel dengan tipe data
    String nama = "Puspita Wanny";
    int umur = 20;
    double tinggi = 155;
    bool isMenikah = false;

    // membuat veriabel dengan kata kunci var
    var alamat = "Jl. Pembangunan Sidirejo Perumahan Grand Fajar Asri No.99, Namorambe, Medan, Sumatera Utara, Indonesia";

    // mencetak variabel
    print("Nama saya $nama. Umur $umur tahun. Tinggi sekitar $tinggi cm.");
    print("Menikah: $isMenikah");
    print("Alamat: $alamat");
}
